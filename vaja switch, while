#include <stdio.h>

int main(void) {
  int x = 4;
  while (x != 0){
    scanf("%d", &x);
    switch(x){
      case 1: printf ("ocena: negativno \n");
      break;
      case 2: printf ("ocena: zadostno \n");
      break;
      case 3: printf ("ocena: dobro \n");
      break;
      case 4: printf ("ocena: prav dobro \n");
      break;
      case 5: printf ("ocena: odliƒçno \n");
      break;
      default: printf("ocena: nepravilna \n");
}
}
}
